#!/bin/sh

test_exit() {
  status=$?
  if test $status -ne 0 
    then 
      exit 
  fi 
}

log="ros.usr.lib.1.12.beta"
dtval=`date +%Y%m%d.%H%M`


$BINPATH/makeall -vb $USR_CODEBASE src.lib/radar${OSTYPE}/ops > $LOGPATH/${log}.${dtval}
test_exit
$BINPATH/makeall -vb $USR_CODEBASE src.lib/radar${OSTYPE}/site/site.$SD_RADARCODE >> $LOGPATH/${log}.${dtval}
test_exit



