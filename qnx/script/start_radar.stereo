#!/bin/sh
set -x
. $HOME/.profile

bin=$HOME/bin
/usr/local/bin/bbcommand -m off
ontty /dev/con4 $bin/errlog 
ontty /dev/con9 $bin/echo_data 

ontty /dev/con3 $bin/fit_write 
ontty /dev/con3 $bin/raw_write -t 3
ontty /dev/con3 $bin/sd_summary 
ontty /dev/con3 $bin/compress -a 
ontty /dev/con3 $bin/gridfit 
ontty /dev/con3 $bin/server -l 1024
ontty /dev/con2 $bin/tdisplay
ontty /dev/con1 $bin/schedule -l /data/scdlogs/scdlog /data/scd/radar.scd


